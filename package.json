{
	"name": "blanked",
	"version": "0.0.0-prototype",
	"description": "An engine designed for making 3D (and in the future, 2D) experiences.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [],
	"bin": {
		"build": "scripts/build.js"
	},
	"files": [
		"dist",
		"src",
		"license.md",
		"tsconfig.json",
		"eslint.shared.js"
	],
	"type": "module",
	"homepage": "https://github.com/james-pre/blanked",
	"author": "James Prevett <jp@jamespre.dev> (https://jamespre.dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/james-pre/blanked.git"
	},
	"bugs": {
		"url": "https://github.com/james-pre/blanked/issues"
	},
	"engines": {
		"node": ">= 22"
	},
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*"
	},
	"scripts": {
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "tsc -p tsconfig.json --noEmit && eslint src",
		"test": "tsx --test --experimental-test-coverage",
		"build": "tsc -p tsconfig.json",
		"build:docs": "typedoc",
		"dev": "npm run build -- --watch",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@types/eslint__js": "^8.42.3",
		"eslint": "^9.9.0",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"typedoc": ">=0.26.6",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.2.0"
	},
	"dependencies": {
		"@babylonjs/core": "^7.22.1",
		"dedent": "^1.6.0",
		"eventemitter3": "^5.0.1",
		"http-status-codes": "^2.3.0",
		"lit": "^3.3.0",
		"logzen": "^0.7.0",
		"socket.io": "^4.8.1",
		"utilium": "^2.3.8"
	}
}
